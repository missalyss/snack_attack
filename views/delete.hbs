{{#each snack}}
<h3>Are you sure you want to delete {{this.name}}?</h3>
<form class="" action="http://localhost:3003/snacks/{{this.id}}?_method=DELETE" method="post">
  <input type="submit" name="delete" value="Yes">
  <button type="button" name="button"><a href="http://localhost:3003/">Go back</a></button>
</form>
{{/each}}
